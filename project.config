[access]
	inheritFrom = All-Projects
[project]
	description = YANG PUBSUB
[receive]
	requireChangeId = true
[access "refs/*"]
	owner = group ldap/gerrit-yang-push-committers
	read = group GitHub Replication
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group ldap/gerrit-yang-push-committers
	label-Verified = -1..+1 group ldap/gerrit-yang-push-committers
	submit = group ldap/gerrit-yang-push-committers
[access "refs/tags/*"]
	create = group ldap/gerrit-yang-push-committers
	push = group ldap/gerrit-yang-push-committers
	pushTag = group ldap/gerrit-yang-push-committers
	pushSignedTag = group ldap/gerrit-yang-push-committers
